  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "jquery-ui-1.8.14.custom.min", "jquery-ui.min", "fullcalendar" %>

  <%= csrf_meta_tag %>
</head>
<body>
<div id="contentPage">
    <div>
