<div id="mainRightReceiving">

  <div class="field">
    <%= f.label :supplier_id %><br />
    <%= f.text_field :supplier_id %>
  </div>
  <div class="field">
    <%= f.label :comment %><br />
    <%= f.text_area:comment, :rows => 3 %>
  </div>
  <div class="field">
    <%= f.hidden_field :receive_type, :value => 1 %>
  </div>
  <div class="actions">
    <%= f.submit "Receive" %>
  </div>

</div>

